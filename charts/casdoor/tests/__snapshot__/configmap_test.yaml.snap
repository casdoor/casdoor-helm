    data:
      app.conf: |
        appname = casdoor
        httpport = 8000
        runmode = dev
        SessionOn = true
        copyrequestbody = true
        driverName = sqlite
        dataSourceName = file:/data/casdoor.db?cache=shared
        dbName = casdoor
        redisEndpoint =
        defaultStorageProvider =
        isCloudIntranet = false
        authState = "casdoor"
        socks5Proxy = ""
        verificationCodeTimeout = 10
        initScore = 0
